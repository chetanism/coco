{
  "name": "api",
  "version": "0.0.0",
  "description": "A nodejs framework",
  "scripts": {
    "test": "jest",
    "lint": "npx eslint \\\"app/**/*.ts\\\"",
    "clear": "rm -rf build",
    "compile": "tsc",
    "build": "npm run clear && npm run compile",
    "start:dev": "PORT=3500 ts-node-dev --respawn --transpile-only --no-notify app/start.ts",
    "start:prod": "npm run build && PORT=3500 NODE_ENV=production node build/start.js",
    "start": "NODE_ENV=production node build/start.js",
    "cli": "ts-node app/src/cli"
  },
  "author": "chetan@clik.ai",
  "license": "ISC",
  "devDependencies": {
    "@types/jest": "^26.0.15",
    "eslint": "^7.12.0",
    "eslint-config-google": "^0.14.0",
    "eslint-plugin-babel": "^5.3.1",
    "jest": "^26.6.1",
    "reflect-metadata": "^0.1.13",
    "ts-jest": "26.4.1",
    "ts-node": "^9.0.0",
    "ts-node-dev": "^1.0.0",
    "typescript": "^4.0.5"
  },
  "dependencies": {
    "command-line-args": "^5.1.1",
    "deepmerge": "^4.2.2"
  }
}
